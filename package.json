    "cypress": "14.0.0",
    "cypress-file-upload": "5.0.8",
    "cypress-xpath": "2.0.1",
    "dayjs": "1.11.13",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-cypress": "^4.2.0",
    "globals": "^16.0.0",
    "luxon": "^3.6.1",
    "moment-timezone": "0.5.47",
    "prettier": "^3.5.3",
    "typescript": "5.4.5",
    "typescript-eslint": "^8.29.0"
  },
